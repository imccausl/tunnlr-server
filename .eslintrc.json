{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": [
        "prettier",
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
        "prettier/prettier": ["error"]
    }
}
